        "lintFilePatterns": [
          "libs/memory/feature-boardgame/src/**/*.ts",
          "libs/memory/feature-boardgame/src/**/*.html"
        ]
      }
    }
  },
  "tags": ["scope:memory", "type:feature"]
}
