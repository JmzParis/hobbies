{
  "projectType": "library",
  "root": "libs/memory/feature-boardgame",
  "sourceRoot": "libs/memory/feature-boardgame/src",
  "prefix": "jz",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/memory/feature-boardgame"],
