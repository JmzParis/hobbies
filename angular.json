{
  "version": 1,
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress",
      "strict": true
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "strict": true
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "defaultProject": "website",
  "projects": {
    "memory": {
      "projectType": "application",
      "root": "apps/memory",
      "sourceRoot": "apps/memory/src",
      "prefix": "jz",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/memory",
            "index": "apps/memory/src/index.html",
            "main": "apps/memory/src/main.ts",
            "polyfills": "apps/memory/src/polyfills.ts",
            "tsConfig": "apps/memory/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/memory/src/favicon.ico",
              "apps/memory/src/.htaccess",
              "apps/memory/src/assets",
              {
                "glob": "**/*",
                "input": "libs/memory/feature-boardgame/src/assets",
                "output": "assets"
              }
            ],
            "styles": [
              "node_modules/primeicons/primeicons.css",
              "node_modules/primeng/resources/themes/saga-blue/theme.css",
              "node_modules/primeng/resources/primeng.min.css",
              "node_modules/primeflex/primeflex.css",
              "apps/memory/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/memory/src/environments/environment.ts",
                  "with": "apps/memory/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "memory:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "memory:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "memory:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/memory/src/**/*.ts",
              "apps/memory/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/memory"],
          "options": {
            "jestConfig": "apps/memory/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "memory-e2e": {
      "root": "apps/memory-e2e",
      "sourceRoot": "apps/memory-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/memory-e2e/cypress.json",
            "tsConfig": "apps/memory-e2e/tsconfig.e2e.json",
            "devServerTarget": "memory:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "memory:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/memory-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
